@using PagedList;
@using PagedList.Mvc;
@model IPagedList<Web_OnlineShop.ModelOnlineShop.User> 

@{
   
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

@foreach(var item in Model)
{
    @item.Name
    <a href="@Url.Action("Edit", "User", new { id= item.ID})">@item.Name</a>
                   
}
@Html.PagedListPager(Model, sotrang => Url.Action("Index", new { page = sotrang }))
